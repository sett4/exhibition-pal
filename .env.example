# Google Sheets OAuth2 refresh token used to obtain access tokens at build time
GOOGLE_SHEETS_REFRESH_TOKEN=
# Spreadsheet identifier that hosts exhibitions data
GOOGLE_SHEETS_SPREADSHEET_ID=
# Range in A1 notation that covers all exhibition columns (15 columns expected)
GOOGLE_SHEETS_RANGE=Exhibitions!A:O
# OAuth2 client credentials (only required in CI/production environments)
GOOGLE_SHEETS_CLIENT_ID=
GOOGLE_SHEETS_CLIENT_SECRET=
# Optional token endpoint override (defaults to Google's endpoint when empty)
GOOGLE_SHEETS_TOKEN_URL=
# Optional log level override for Winston performance metrics
LOG_LEVEL=
# Artwork spreadsheet configuration for Stand.fm listings (14 columns A:N)
GOOGLE_ARTWORK_SPREADSHEET_ID=
GOOGLE_ARTWORK_RANGE=
